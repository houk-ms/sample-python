{
	"projectType": "azd",
	"resources": [
	  {
		"type": "azurecontainerapp",
		"instances": [{
			"name": "samplepython"
		}]
	  }
    ],
	"services": [
		{
		  "host": "${azurecontainerapp.samplepython}",
		  "language": "python",
		  "project": "."
		}
	],
	"settings": [{
		"resource": "${azurecontainerapp.samplepython}",
		"envs": [{
			"name": "PORT",
			"value": "80"
		}]
	}]
}